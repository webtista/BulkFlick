{
  "name": "bulkflick",
  "version": "0.1.0",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./src/styles.css -o ./public/assets/styles.css",
    "css:watch": "npx @tailwindcss/cli -i ./src/styles.css -o ./public/assets/styles.css --watch",
    "dev:electron": "electron .",
    "dev": "npm-run-all --parallel css:watch dev:electron"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "electron": "^38.2.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {}
}
